filters:
  and:
    - type == "head"
views:
  - type: table
    name: Table
    filters:
      and:
        - file.folder != "templates"
    order:
      - file.name
      - status
    sort:
      - property: file.ctime
        direction: DESC
    columnSize:
      file.name: 428
  - type: cards
    name: View
    cardSize: 180
    imageAspectRatio: 0.3
